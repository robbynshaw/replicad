{
  "name": "replicad-viewer-vscode",
  "publisher": "replicad",
  "displayName": "Replicad Viewer",
  "description": "View replicad scripts in VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robbynshaw/replicad"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "replicad-viewer-vscode.renderReplicad",
        "title": "Open Replicad Visualiser"
      }
    ]
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build:webview": "tsc --project ./tsconfig.webview.json && vite build",
    "build:extension": "tsc --project ./tsconfig.extension.json",
    "build": "pnpm run build:webview && pnpm run build:extension",
    "vscode:prepublish": "pnpm run build",
    "watch": "tsc -watch -p ./tsconfig.extension.json",
    "pretest": "pnpm run build && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "vsce package --no-dependencies"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "replicad": "workspace:^0.15.3",
    "replicad-opencascadejs": "workspace:^0.15.2",
    "replicad-react": "workspace:^",
    "styled-components": "^6.1.8",
    "three": ">=0.155.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "2.22.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rmraf": "^1.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
